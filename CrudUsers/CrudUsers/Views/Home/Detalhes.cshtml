@model User

<div class="container">
	<h1>Editar de Usuários</h1>

	<input type="hidden" asp-for="Id" />
	<div class="mt-3">
	
		<input type="text" class="form-control" id="name" name="name"  asp-for="UserName" disabled />
	</div>

	<div class="mt-3">
		<label for="lastName" class="form-label">Sobrenome:</label>
		<input type="text" class="form-control" id="lastName" name="lastName" asp-for="LastName" disabled />
	</div>

	<div class="mt-3">
		<label for="email" class="form-label">E-mail:</label>
		<input type="email" class="form-control" id="email" name="email" asp-for="Email" disabled />
	</div>

	<div class="mt-3">
		<label for="position" class="form-label">Cargo:</label>
		<input type="text" class="form-control" id="position" name="position" asp-for="Position" disabled />
	</div>

	<div class="d-flex mt-5 g2">
		<a asp-action="Index" asp-controller="Home" class="btn btn-info w-100 mx-2">Voltar</a>
	</div>

</div>